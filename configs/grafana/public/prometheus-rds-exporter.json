         "tags": [
            "dmf",
            "database"
         ],
         "title": "Database",
         "type": "dashboards"
      },
      {
         "asDropdown": true,
         "includeVars": true,
         "keepTime": true,
         "tags": [
            "dmf",
            "table"
         ],
         "title": "Table",
         "type": "dashboards"
      },
      {
         "asDropdown": true,
         "includeVars": true,
